/* Configuration for GH commits adding certain code snippets. */
{
  "name": "gh_repo_path_commits",
  "input_parameters": ["repo_name", "path"],
  "validation_parameters": ["code_block", "code_block_normalized"],
  "output_parameter_mapping": {
    "commits": [
      "*",
      {
        "commit_sha": ["sha"],
        "commit_date": ["commit", "author", "date"]
      }
    ]
  },
  "pre_request_callbacks": ["validate_code_block_normalization"],
  "post_request_callbacks": ["extract_parameters", "sort_commits"],
  "uri_template": "https://api.github.com/repos/{repo_name}/commits?path={path}&access_token={api_key}",
  "api_key": "", // add API key here
  "ignore_duplicates": false,
  "delay": [40, 1000]
}
