/* Configuration for Google Custom Search
 * (see https://developers.google.com/custom-search/json-api/v1/using_rest)
 * Currenty, only the top 10 results are saved (support for pagination needed to retrieve the rest).
 */
{
  "name": "google_query___search-results",
  "input_parameters": ["query"],
  "ignore_input_duplicates": false,
  "uri_template": "https://www.googleapis.com/customsearch/v1?key={api_key_1}&cx={api_key_2}&q={query}",
  "headers": {},
  "api_keys": [
    "", // add API key here
    "" // add search engine id here
  ],
  "delay": [40, 1000],
  "pre_request_callbacks": [],
  "output_parameter_mapping": {
    "result": ["items", "*", {
      "title": ["title"],
      "url": ["link"]
    }]
  },
  "apply_output_filter": false,
  "post_request_callbacks": [],
  "flatten_output": true,
  "chained_request": {}
}
